{
  "name": "membership",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "prepare": "husky install",
    "add:lint-staged": "npx mrm@2 lint-staged",
    "hooks:pre-commit": "npm run add:lint-staged && npx husky add .husky/pre-commit 'npx lint-staged'",
    "hooks:init": "npm run prepare && npm run hooks:pre-commit",
    "eslint:init": "eslint --init",
    "bootstrap": "npm run hooks:init && npm run eslint:init",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^1.5.2",
    "@middy/http-json-body-parser": "^1.5.2",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.55.0",
    "@shelf/jest-dynamodb": "^2.1.0",
    "@types/aws-lambda": "^8.10.83",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.17.14",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^6.0.0",
    "jest": "^27.1.0",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^11.0.0",
    "serverless": "^2.23.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.0.0",
    "serverless-plugin-typescript": "^2.0.0",
    "serverless-webpack": "^5.5.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.4.2",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
